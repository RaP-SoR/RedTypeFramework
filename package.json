{
  "name": "redtypeframework",
  "version": "0.0.1",
  "description": "RedTypeFramework(RTF) is a framework for creating RedM resources with TypeScript and Vue 3",
  "author": "RaPSoR",
  "private": true,
  "workspaces": [
    "core",
    "modules/*",
    "resources/*",
    "tools/*",
    "rtf-ui"
  ],
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:core && yarn build:ui",
    "dev": "yarn dev:all",
    "dev:all": "concurrently \"yarn dev:core\" \"yarn dev:ui\"",
    "dev:core": "vite build --watch",
    "dev:ui": "yarn workspace rtf-ui dev",
    "build:core": "vite build",
    "build:ui": "yarn workspace rtf-ui build",
    "watch": "concurrently \"yarn watch:core\" \"yarn watch:ui\"",
    "watch:core": "vite build --watch",
    "watch:ui": "yarn workspace rtf-ui watch",
    "preview": "yarn workspace rtf-ui preview",
    "type-check": "vue-tsc --noEmit && yarn workspace rtf-ui type-check"
  },
  "dependencies": {
    "@citizenfx/client": "^2.0.14862-1",
    "@citizenfx/server": "^2.0.14862-1",
    "ts-node": "^10.9.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@types/node": "^16.18.38",
    "@vitejs/plugin-vue": "^4.2.3",
    "concurrently": "^8.2.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-vuetify": "1.0.2",
    "vue-tsc": "^2.2.10"
  },
  "resolutions": {
    "unplugin": "1.6.0",
    "unplugin-auto-import": "0.16.7",
    "unplugin-vue-components": "0.25.2",
    "unplugin-fonts": "1.0.3"
  },
  "engines": {
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  }
}
